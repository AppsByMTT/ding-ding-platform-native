{
  "expo": {
    "name": "ding-ding",
    "slug": "ding-ding",
    "version": "1.0.0",
    "icon": "./src/assets/images/applogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "orientation": "default",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.underpin.dingding"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/applogo.png",
        "backgroundColor": "#FFB302"
      },
      "package": "com.underpin.dingding"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ff3702db-2b39-4340-a46c-ca66d6d88e0e"    
        }
    }
  }
}